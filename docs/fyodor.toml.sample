# fyodor sample config file

# Set the translations you get on your clippings file
[parser]
highlight = "Your Highlight"
note = "Your Note"
bookmark = "Your Bookmark"
clip = "Clip This Article"
loc = "Location"
page = "page"
time = "Added on"

[output]
extension = "md"
# Whether we should print each entry time next to them
time = false
